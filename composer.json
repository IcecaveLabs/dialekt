            "email": "mailjamesharris@gmail.com",
            "homepage": "https://github.com/jmalloc"
        }
    ],
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2",
        "phake/phake": "^3",
        "phpunit/phpunit": "^8"
    },
    "autoload": { "psr-4": { "Dialekt\\": "src" } },
